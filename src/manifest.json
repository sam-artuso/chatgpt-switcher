{
  "manifest_version": 3,
  "name": "Custom ChatGPT switcher",
  "version": "1.0.5",
  "description": "Quickly switch between custom GPTs on ChatGPT.com using a keyboard shortcut. Fuzzy search with autocomplete menu.",
  "author": "Samuele Artuso",
  "homepage_url": "https://github.com/kyuzz/chatgpt-switcher",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}