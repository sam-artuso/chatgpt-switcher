{
  "manifest_version": 3,
  "name": "ChatGPT Switcher",
  "version": "1.0",
  "description": "Quickly switch between custom GPTs on ChatGPT",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}